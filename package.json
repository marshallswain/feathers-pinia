{
  "name": "@gabortorma/feathers-pinia",
  "type": "module",
  "version": "5.0.1",
  "packageManager": "pnpm@10.14.0",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/feathers-pinia.js"
      },
      "require": "./dist/feathers-pinia.umd.cjs"
    },
    "./ufuzzy": {
      "import": "./dist/ufuzzy.js",
      "require": "./dist/ufuzzy.cjs"
    }
  },
  "main": "./dist/feathers-pinia.umd.cjs",
  "module": "./dist/feathers-pinia.js",
  "types": "dist/",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs; npm run docs:copy-assets",
    "docs:serve": "vitepress serve docs --port 5001",
    "docs:copy-assets": "cp ./docs/feathers-pinia-bird.svg ./docs/.vitepress/dist/",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:viz": "npx vite-bundle-visualizer",
    "prepare": "npm run test && npm run build",
    "serve": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "publish": "git push origin --tags && git push origin",
    "release:pre": "npm run build && npm version prerelease && npm publish --tag pre",
    "release:patch": "npm run build && npm version patch && npm publish --access public",
    "release:minor": "npm run build && npm version minor && npm publish --access public",
    "release:major": "npm run build && npm version major && npm publish --access public",
    "spell-check": "cspell-cli '{src,docs,tests,example}/**/*'",
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix . src/ tests/ docs/",
    "sfl": "npm run spell-check && npm run format && npm run lint"
  },
  "peerDependencies": {
    "@feathersjs/adapter-commons": "^5.0.0",
    "@feathersjs/commons": "^5.0.0",
    "@feathersjs/feathers": "^5.0.0",
    "@leeoniya/ufuzzy": "^1.0.14",
    "pinia": "^3.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "@leeoniya/ufuzzy": {
      "optional": true
    }
  },
  "dependencies": {
    "@vueuse/core": "^13.6.0",
    "bson-objectid": "^2.0.4",
    "events": "^3.3.0",
    "fast-copy": "^3.0.2",
    "fast-deep-equal": "^3.1.3",
    "fast-json-stable-stringify": "^2.1.0",
    "feathers-pinia": "link:",
    "isomorphic-mongo-objectid": "^1.0.10",
    "just-debounce": "^1.1.0",
    "jwt-decode": "^4.0.0",
    "lz-string": "^1.5.0",
    "sift": "^17.1.3",
    "vue-demi": "latest"
  },
  "optionalDependencies": {
    "@feathersjs/rest-client": "5.0.33"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@babel/types": "^7.28.2",
    "@cspell/dict-fr-fr": "^2.3.2",
    "@faker-js/faker": "^9.9.0",
    "@feathersjs/adapter-commons": "^5.0.34",
    "@feathersjs/authentication-client": "^5.0.34",
    "@feathersjs/commons": "^5.0.34",
    "@feathersjs/feathers": "^5.0.34",
    "@feathersjs/memory": "^5.0.34",
    "@feathersjs/rest-client": "^5.0.34",
    "@feathersjs/typebox": "^5.0.34",
    "@leeoniya/ufuzzy": "^1.0.18",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.2.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "3.1.2",
    "@vue/compiler-sfc": "^3.5.18",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "cspell-cli": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "flexsearch": "^0.8.205",
    "markdown-it": "^14.1.0",
    "path": "^0.12.7",
    "pinia": "^3.0.3",
    "postcss-nested": "^7.0.2",
    "shx": "^0.4.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "tailwindcss"
      ]
    }
  }
}
